<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="better_compatibility" xsi:type="array">
                    <item name="Amasty_Xnotif" xsi:type="boolean">false</item>
                </argument>
                <argument name="bundles" xsi:type="array">
                    <item name="default" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="amnotification" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyXnotif/js/amnotification</item>
                            </item>
                            <item name="Amasty_Xnotif/js/category_subscribe" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyXnotif/js/category-subscribe</item>
                            </item>
                            <item name="Amasty_Xnotif/js/category/configurable" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyXnotif/js/category-configurable</item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
